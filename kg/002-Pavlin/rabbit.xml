<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE SimpleVectorGraphics>
<Shapes>
	<!-- Закрытый залитый многоугольник -->
	<Path closed="true" fill="#FF000080" stroke="#0000FF" stroke-width="3">
		<Point x="50" y="50"/>
        <Point x="150" y="75"/>
        <Point x="100" y="150"/>
	</Path>

    <!-- Незамкнутая линия с обводкой -->
    <Path closed="false" stroke="#00FF00" stroke-width="2">
        <Point x="200" y="50"/>
        <Point x="250" y="200"/>
        <Point x="300" y="100"/>
    </Path>

    <!-- Залитый треугольник -->
    <Path closed="true" fill="#FFFF00" stroke="#000000">
        <Point x="350" y="50"/>
        <Point x="400" y="150"/>
        <Point x="300" y="100"/>
		</Path>

    <Path closed="true" stroke="#000000">
        <Point x="450" y="50"/>
        <Point x="500" y="150"/>
        <Point x="400" y="100"/>
    </Path>


    <Path closed="true" fill="#FFFF00" stroke-width="0">
        <Point x="550" y="50"/>
        <Point x="600" y="150"/>
        <Point x="500" y="100"/>
    </Path>
</Shapes>
